then :

  # Extract the first word of "addr2line", so it can be a program name with args.
set dummy addr2line; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_works+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$works"; then
  ac_cv_prog_works="$works" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_works="yes"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_prog_works" && ac_cv_prog_works="no"
fi
fi
works=$ac_cv_prog_works
if test -n "$works"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $works" >&5
printf "%s\n" "$works" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi



  if test $works = yes
then :


  for ac_func in popen backtrace
do :
  as_ac_var=`printf "%s\n" "ac_cv_func_$ac_func" | $as_tr_sh`
ac_fn_cxx_check_func "$LINENO" "$ac_func" "$as_ac_var"
if eval test \"x\$"$as_ac_var"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_func" | $as_tr_cpp` 1
_ACEOF
 works=yes
else $as_nop
  works=no; break
fi

done

fi

  if test $works = yes
then :

           for ac_header in execinfo.h dlfcn.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_cxx_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 works=yes
else $as_nop
  works=no; break
fi

done

fi

  if test $works = yes
then :

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for library containing dladdr" >&5
printf %s "checking for library containing dladdr... " >&6; }
if test ${ac_cv_search_dladdr+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_func_search_save_LIBS=$LIBS
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

namespace conftest {
  extern "C" int dladdr ();
}
int
main (void)
{
return conftest::dladdr ();
  ;
  return 0;
}
_ACEOF
for ac_lib in '' dl
do
  if test -z "$ac_lib"; then
    ac_res="none required"
  else
    ac_res=-l$ac_lib
    LIBS="-l$ac_lib  $ac_func_search_save_LIBS"
  fi
  if ac_fn_cxx_try_link "$LINENO"
then :
  ac_cv_search_dladdr=$ac_res
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext
  if test ${ac_cv_search_dladdr+y}
