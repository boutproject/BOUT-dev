    fftw_wisdom0=`$as_dirname -- "$fftw_path" ||
$as_expr X"$fftw_path" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$fftw_path" : 'X\(//\)[^/]' \| \
	 X"$fftw_path" : 'X\(//\)$' \| \
	 X"$fftw_path" : 'X\(/\)' \| . 2>/dev/null ||
printf "%s\n" X"$fftw_path" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
    fftw_wisdom=`$as_dirname -- "$fftw_wisdom0" ||
$as_expr X"$fftw_wisdom0" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$fftw_wisdom0" : 'X\(//\)[^/]' \| \
	 X"$fftw_wisdom0" : 'X\(//\)$' \| \
	 X"$fftw_wisdom0" : 'X\(/\)' \| . 2>/dev/null ||
printf "%s\n" X"$fftw_wisdom0" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
