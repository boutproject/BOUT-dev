eval ac_res=\$$as_CACHEVAR
	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
$as_echo "$ac_res" >&6; }
if eval test \"x\$"$as_CACHEVAR"\" = x"yes"; then :

if ${CXXFLAGS+:} false; then :

  case " $CXXFLAGS " in #(
  *" $flag "*) :
    { { $as_echo "$as_me:${as_lineno-$LINENO}: : CXXFLAGS already contains \$flag"; } >&5
