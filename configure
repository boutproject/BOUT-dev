    if test "x$with_pnetcdf" = "xyes"
then :

      # Extract the first word of "ncmpidump", so it can be a program name with args.
set dummy ncmpidump; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_path_NCMPIDUMP_PATH+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  case $NCMPIDUMP_PATH in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_NCMPIDUMP_PATH="$NCMPIDUMP_PATH" # Let the user override the test with a path.
  ;;
