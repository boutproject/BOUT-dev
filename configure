rm conftest.$ac_ext
LIBS=$ac_func_search_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_search_dladdr" >&5
printf "%s\n" "$ac_cv_search_dladdr" >&6; }
ac_res=$ac_cv_search_dladdr
if test "$ac_res" != no
