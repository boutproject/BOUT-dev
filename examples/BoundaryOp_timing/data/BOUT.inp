[mesh]
nx = 128
ny = 128
nz = 128

ixseps1 = -1

[f_dirichlet]
bndry_all = dirichlet

[f_neumann]
bndry_all = neumann

[f_dirichlet_o3]
bndry_all = dirichlet_o3

[f_dirichlet_val]
bndry_all = dirichlet(42.)

[f_neumann_val]
bndry_all = neumann(42.)

[f_dirichlet_o3_val]
bndry_all = dirichlet_o3(42.)

[f_dirichlet_expr]
bndry_all = dirichlet(x+y+z)

[f_neumann_expr]
bndry_all = neumann(x+y+z)

[f_dirichlet_o3_expr]
bndry_all = dirichlet_o3(x+y+z)
