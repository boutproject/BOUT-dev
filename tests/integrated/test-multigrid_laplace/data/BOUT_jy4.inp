#############################################

[f1]
# zero value x boundary conditions
p1 = 0.39503274
q1 = 0.20974396
function = sin(2*pi*3*x)*cos(z-p1) + sin(2*pi*x)*cos(3*z-q1)

[d1]
r1 = 0.512547
s1 = 0.30908712
function = 1. + .1*cos(4.*x-r1)*sin(3*z-s1)

[c1]
